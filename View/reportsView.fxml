<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.reportsViewController">
   <children>
      <Label layoutX="14.0" layoutY="22.0" text="Reports">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="returnToMain" layoutX="636.0" layoutY="450.0" mnemonicParsing="false" onAction="#returnClick" text="Return to Main Screen" />
      <TextArea fx:id="outputText" layoutX="340.0" layoutY="61.0" prefHeight="378.0" prefWidth="431.0" />
      <Button fx:id="appointmentCounts" layoutX="19.0" layoutY="99.0" mnemonicParsing="false" onAction="#countTypesClick" prefHeight="25.0" prefWidth="298.0" text="Generate Appointment Type Count/ Month" />
      <ComboBox fx:id="contacts" layoutX="19.0" layoutY="386.0" onAction="#getContactSchedules" prefHeight="25.0" prefWidth="243.0" promptText="Select a contact to generate schedule:" />
      <ComboBox fx:id="customers" layoutX="19.0" layoutY="244.0" onAction="#getApptCount" prefHeight="25.0" prefWidth="311.0" promptText="Select Customer to Count Appointments/ Month" />
      <Label fx:id="label2" layoutX="14.0" layoutY="221.0" prefHeight="17.0" prefWidth="304.0" />
      <Label fx:id="label3" layoutX="19.0" layoutY="356.0" prefHeight="17.0" prefWidth="304.0" />
   </children>
</AnchorPane>
